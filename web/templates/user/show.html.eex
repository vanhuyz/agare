<h2>Show user</h2>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @user.name %>
  </li>

  <li>
    <strong>Email:</strong>
    <%= @user.email %>
  </li>

  <li>
    <strong>Image:</strong>
    <%= @user.image %>
  </li>

  <li>
    <strong>Provider:</strong>
    <%= @user.provider %>
  </li>

  <li>
    <strong>Uid:</strong>
    <%= @user.uid %>
  </li>

</ul>

<%= link "Edit", to: user_path(@conn, :edit, @user) %>
<%= link "Back", to: user_path(@conn, :index) %>
